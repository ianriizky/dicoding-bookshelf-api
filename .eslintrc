{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "class-methods-use-this": "off",
    "comma-dangle": "off",
    "implicit-arrow-linebreak": "off",
    "max-len": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off",
    "operator-linebreak": "off"
  }
}
